    "n = int(input())\n",
    "\n",
    "def solution(a):\n",
    "    if a == 2:\n",
    "        return 2\n",
    "    elif a == 3:\n",
    "        return 3\n",
    "    else:\n",
    "        return solution(a-1) + solution(a-2)\n",
    "    \n",
    "print(solution(n))"
