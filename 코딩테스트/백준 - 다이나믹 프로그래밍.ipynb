   "outputs": [],
   "source": [
    "n = int(input())\n",
    "count = 0\n",
    "while n > 0:\n",
    "    if n % 5 != 0:\n",
    "        n -= 3\n",
    "        count += 1\n",
    "    else:\n",
    "        count += n//5\n",
    "        break\n",
    "\n",
    "if n < 0:\n",
    "    count = -1\n",
    "print(count)"
   ]
  }
